{
  "builders": [
    {
      "type": "docker",
      "image": "consul:dns",
      "export_path": "consul-env.tar",
      "pull": false
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "inline": [
        "wget --no-check-certificate https://github.com/hashicorp/envconsul/releases/download/v0.2.0/envconsul_linux_amd64",
        "chmod 755 ./envconsul_linux_amd64"
      ]
    }
  ]
}
